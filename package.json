{
  "name": "itzpapa",
  "description": "An Astro blog template with Obsidian-style features (WikiLink, Callout, Mark Highlight)",
  "type": "module",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hachian/itzpapa.git"
  },
  "keywords": [
    "astro",
    "blog",
    "obsidian",
    "wikilink",
    "markdown",
    "static-site-generator"
  ],
  "author": "hachian",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "node tests/test-runner.js",
    "test:unit": "node --test tests/unit/wikilink-unit-test.js tests/unit/mark-highlight-unit-test.js tests/unit/tags-unit-test.js tests/unit/callout-test.js",
    "test:integration": "node --test tests/integration/integration-test.js",
    "test:e2e": "node --test tests/e2e/e2e-test.js tests/e2e/html-validator-test.js",
    "test:wikilink": "node tests/integration/wikilink-test.js",
    "test:image": "node --test tests/integration/image-wikilink-test.js",
    "test:table": "node tests/integration/table-wikilink-test.js",
    "test:performance": "node tests/performance/performance-test.js",
    "test:all": "npm run test:wikilink && npm run test:image && npm run test:table",
    "test:fixtures": "node --test tests/utils/fixture-loader-test.js"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "astro": "^5.13.7",
    "remark-breaks": "^4.0.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "cheerio": "^1.1.2",
    "remark": "^15.0.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  }
}
